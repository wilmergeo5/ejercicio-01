from pylab import*

x0 = 0.0
x1 = 2.0
dx = 0.001
n = int(ceil((x1-x0)/dx)+1)
x = zeros((n,1),float)
y1 = zeros((n,1),float)
y2 = zeros((n,1),float)
m = 0.8

for i in range (n):
    x[i] = x0 + i*dx
    y1[i] = (sqrt((2*x)/m))[i]
    y2[i] = (sqrt(1 - ((2*x)/3 + 1)**-2))[i]

axhline(y=1)  
plot(x,y1)
plot(x,y2)
xlabel('k/mc^2')
ylabel('v')
grid()
show()
